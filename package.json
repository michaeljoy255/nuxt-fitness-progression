{
  "name": "nuxt-boilerplate-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest",
    "test:update": "jest -u",
    "new": "cross-env HYGEN_TMPLS=generators hygen new"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "5.12.x",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "nuxt": "2.14.x",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "3.1.x",
    "@nuxtjs/eslint-module": "2.0.x",
    "@nuxtjs/stylelint-module": "4.0.x",
    "@nuxtjs/vuetify": "1.11.x",
    "@vue/test-utils": "1.0.x",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.x",
    "babel-jest": "26.1.x",
    "cross-env": "^7.0.2",
    "eslint": "7.5.x",
    "eslint-config-prettier": "6.11.x",
    "eslint-plugin-nuxt": "1.0.x",
    "eslint-plugin-prettier": "3.1.x",
    "husky": "4.2.x",
    "hygen": "6.0.x",
    "jest": "26.1.x",
    "lint-staged": "10.2.x",
    "prettier": "2.0.x",
    "stylelint": "13.6.x",
    "stylelint-config-prettier": "8.0.x",
    "stylelint-config-standard": "20.0.x",
    "vue-jest": "3.0.x"
  },
  "engines": {
    "node": ">=10.13.3",
    "yarn": ">=1.0.0"
  }
}
